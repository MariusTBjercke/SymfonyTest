{% extends "grid/base.html.twig" %}
{% set css = "home" %}

{% block title %}Home{% endblock %}

{% block maincontent %}

    <div class="container">
        <div class="row">
            {% for message in app.flashes('success') %}
                <div class="alert alert-success">
                    {{ message }}
                </div>
            {% endfor %}
        </div>
    </div>

    {{ language }}

    <div class="container">
        <div class="row">
            <div class="col">
                {% if user is not null %} {{ user.username }} {% endif %}
            </div>
        </div>
    </div>

{% endblock %}
